{
  "name": "@rehearsal/reporter",
  "version": "0.0.13",
  "description": "Rehearsal Reporter",
  "keywords": [
    "rehearsal",
    "reporter",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rehearsal-js/rehearsal-js.git"
  },
  "license": "BSD-2-Clause",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn tsc -b",
    "lint": "yarn npm-run-all lint:*",
    "lint:eslint": "yarn eslint . --ext .ts",
    "lint:tsc-src": "yarn tsc --noEmit",
    "prepare": "yarn build",
    "test": "yarn lint && yarn nyc mocha"
  },
  "nyc": {
    "extension": [
      ".ts"
    ]
  },
  "dependencies": {
    "debug": "^4.3.3",
    "fs-extra": "^10.0.1",
    "json5": "^2.2.0",
    "tmp": "^0.2.1",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/json5": "*",
    "chai": "^4.3.6",
    "mocha": "^9.2.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "volta": {
    "node": "16.14.0",
    "yarn": "1.22.17"
  }
}
