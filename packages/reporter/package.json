{
  "name": "@rehearsal/reporter",
  "version": "1.0.10-beta",
  "description": "Rehearsal Reporter",
  "keywords": [
    "rehearsal",
    "reporter",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rehearsal-js/rehearsal-js.git"
  },
  "license": "BSD-2-Clause",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./*": "./*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint --fix . --ext .ts",
    "lint:tsc-src": "tsc --noEmit",
    "lint:tsc-test": "tsc --noEmit --project test/tsconfig.json",
    "test": "vitest --run",
    "test:watch": "vitest --coverage --watch",
    "version": "pnpm version"
  },
  "dependencies": {
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/eslint": "^8.21.0",
    "@types/lodash.get": "^4.4.7",
    "@types/sarif": "^2.1.4",
    "fs-extra": "^11.1.0",
    "lodash.get": "^4.4.2",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5",
    "vitest-mock-extended": "^1.0.9"
  },
  "packageManager": "pnpm@7.12.1",
  "engines": {
    "node": ">=14.16.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
