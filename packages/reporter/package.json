  "dependencies": {
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/eslint": "^8.4.1",
    "@types/lodash.get": "^4.4.7",
    "@types/sarif": "^2.1.4",
    "lodash.get": "^4.4.2",
    "typescript": "4.7.4",
    "vitest": "^0.23.4",
    "vitest-mock-extended": "^0.1.15"
  },
  "packageManager": "pnpm@7.12.1",
  "engines": {
    "node": ">=14.16.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
