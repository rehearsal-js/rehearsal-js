{
  "name": "@rehearsal/cli",
  "version": "0.0.33",
  "description": "Rehearsal CLI",
  "repository": {
    "type": "git",
    "url": "https://github.com/rehearsal-js/rehearsal-js.git"
  },
  "license": "BSD-2-Clause",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "rehearsal": "./bin/rehearsal.js"
  },
  "files": [
    "bin",
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -b",
    "lint": "run -T run-p 'lint:*'",
    "lint:eslint": "run -T eslint --fix . --ext .ts",
    "lint:tsc-src": "tsc --noEmit",
    "prepack": "yarn build",
    "test": "run -T vitest run",
    "test:watch": "run -T vitest --coverage --watch",
    "dev:clean": "rm test/fixtures/app/.rehearsal.json && git restore test/fixtures/app/foo test/fixtures/app/foo_2 package.json ../../yarn.lock"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@microsoft/api-documenter": "^7.13.68",
    "@rehearsal/migrate": "workspace:*",
    "@rehearsal/reporter": "workspace:*",
    "@rehearsal/upgrade": "workspace:*",
    "compare-versions": "^5.0.1",
    "debug": "^4.3.2",
    "dependency-cruiser": "^11.15.0",
    "execa": "^5.1.1",
    "findup-sync": "^5.0.0",
    "fs-extra": "^10.0.0",
    "glob": "^8.0.3",
    "json5": "^2.2.0",
    "listr2": "^5.0.1",
    "semver": "^7.3.5",
    "simple-git": "^3.10.0",
    "tmp": "^0.2.1",
    "toposort": "^2.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">=14.16.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
