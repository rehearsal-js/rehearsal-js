{
  "name": "@rehearsal-js/cli",
  "version": "0.0.1",
  "description": "Rehearsal-JS CLI",
  "repository": {
    "type": "git",
    "url": "https://github.com/rehearsal-js/rehearsal-js.git"
  },
  "license": "BSD-2-Clause",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "rehearsal": "./bin/run"
  },
  "files": [
    "bin",
    "dist",
    "src",
    "oclif.manifest.json"
  ],
  "scripts": {
    "build": "yarn tsc -b",
    "docs": "yarn api-extractor run --typescript-compiler-folder ./node_modules/typescript && yarn api-documenter markdown -i ./temp -o ./docs",
    "lint": "eslint -c .eslintrc.js --ext .ts .",
    "oclif:manifest": "yarn oclif-dev manifest",
    "oclif:readme": "yarn oclif-dev readme",
    "oclif:markdown": "oclif-dev readme --dir ./markdown --multi",
    "prepack": "yarn docs",
    "prepare": "yarn build",
    "test": "yarn lint && nyc --extension .ts mocha \"test/commands/**/*.test.ts\" \"test/helpers/**/*.test.ts\""
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "nyc": {
    "extension": [
      ".ts"
    ]
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.5",
    "@oclif/parser": "^3.8.5",
    "@oclif/plugin-help": "^3.2.4",
    "@rehearsal-js/plugin-ts-migrate": "^0.0.1",
    "@rehearsal-js/reporter": "^0.0.1",
    "@rehearsal-js/tsc-transforms": "^0.0.1",
    "compare-versions": "^4.1.1",
    "debug": "^4.3.2",
    "execa": "^6.0.0",
    "fs-extra": "^10.0.0",
    "jscodeshift": "^0.13.0",
    "json5": "^2.2.0",
    "listr2": "^3.13.4",
    "tmp": "^0.2.1",
    "ts-migrate": "^0.1.26",
    "ts-migrate-plugins": "^0.1.26",
    "ts-migrate-server": "^0.1.26",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@microsoft/api-documenter": "^7.13.68",
    "@microsoft/api-extractor": "^7.18.19",
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.22",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/jscodeshift": "^0.11.3",
    "@types/json5": "*",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.7",
    "@types/tmp": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "chai": "^4.3.4",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-oclif": "^0.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "mocha": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "typescript": "~4.5.2"
  },
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0"
  },
  "oclif": {
    "commands": "./dist/src/commands",
    "bin": "rehearsal",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "volta": {
    "node": "16.0.0",
    "yarn": "1.22.17"
  }
}
