{
  "name": "@rehearsal/cli",
  "version": "1.0.6-beta",
  "description": "Rehearsal CLI",
  "repository": {
    "type": "git",
    "url": "https://github.com/rehearsal-js/rehearsal-js.git"
  },
  "license": "BSD-2-Clause",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "rehearsal": "./bin/rehearsal.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "docs": "api-extractor run --typescript-compiler-folder ./node_modules/typescript && api-documenter markdown -i ./temp -o ./docs",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint --fix . --ext .ts",
    "lint:tsc-src": "tsc --noEmit",
    "lint:tsc-test": "tsc --noEmit --project test/tsconfig.json",
    "test": "vitest --run",
    "test:watch": "vitest --coverage --watch",
    "version": "pnpm version"
  },
  "dependencies": {
    "@microsoft/api-documenter": "^7.21.3",
    "@rehearsal/migrate": "workspace:*",
    "@rehearsal/migration-graph": "workspace:*",
    "@rehearsal/regen": "workspace:*",
    "@rehearsal/reporter": "workspace:*",
    "@rehearsal/upgrade": "workspace:*",
    "@rehearsal/utils": "workspace:*",
    "chalk": "^4.1.2",
    "commander": "^10.0.0",
    "compare-versions": "^5.0.3",
    "cosmiconfig": "^8.0.0",
    "debug": "^4.3.4",
    "enquirer": "^2.3.6",
    "eslint": "^8.33.0",
    "execa": "^5.1.1",
    "fs-extra": "^11.1.0",
    "listr2": "^5.0.7",
    "simple-git": "^3.16.0",
    "tmp": "^0.2.1",
    "typescript": "^4.9.5",
    "which": "^3.0.0",
    "winston": "^3.8.2",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "@types/which": "^2.0.1",
    "vitest": "^0.28.4"
  },
  "packageManager": "pnpm@7.12.1",
  "engines": {
    "node": ">=14.16.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
