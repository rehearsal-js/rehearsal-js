{
  "name": "@rehearsal/cli",
  "version": "0.0.27",
  "description": "Rehearsal CLI",
  "repository": {
    "type": "git",
    "url": "https://github.com/rehearsal-js/rehearsal-js.git"
  },
  "license": "BSD-2-Clause",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "rehearsal": "./bin/run"
  },
  "files": [
    "bin",
    "dist",
    "src",
    "oclif.manifest.json"
  ],
  "scripts": {
    "build": "yarn tsc -b",
    "docs": "yarn api-extractor run --typescript-compiler-folder ./node_modules/typescript && yarn api-documenter markdown -i ./temp -o ./docs",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "yarn eslint --fix . --ext .ts",
    "lint:tsc-src": "yarn tsc --noEmit",
    "oclif:manifest": "yarn oclif-dev manifest",
    "oclif:readme": "yarn oclif-dev readme",
    "oclif:markdown": "oclif-dev readme --dir ./markdown --multi",
    "prepare": "yarn build",
    "test": "yarn nyc mocha",
    "dev:clean": "rm test/fixtures/app/.rehearsal.json && git restore test/fixtures/app/foo test/fixtures/app/foo_2 package.json ../../yarn.lock && yarn add -D typescript@4.2.4 --ignore-scripts && yarn build"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "nyc": {
    "extension": [
      ".ts"
    ]
  },
  "dependencies": {
    "@microsoft/api-documenter": "^7.13.68",
    "@oclif/core": "^1.12.1",
    "@rehearsal/migrate": "^0.0.27",
    "@rehearsal/reporter": "^0.0.27",
    "compare-versions": "^4.1.1",
    "debug": "^4.3.2",
    "execa": "^6.1.0",
    "find-up": "^6.3.0",
    "fs-extra": "^10.0.0",
    "json5": "^2.2.0",
    "listr2": "^5.0.1",
    "semver": "^7.3.5",
    "simple-git": "^3.10.0",
    "tmp": "^0.2.1",
    "winston": "^3.3.3",
    "@oclif/plugin-version": "^1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.2",
    "@microsoft/api-documenter": "^7.13.68",
    "@microsoft/api-extractor": "^7.18.19",
    "@types/chai": "^4.2.17",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/json5": "*",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18.0.3",
    "@types/semver": "^7.3.9",
    "@types/tmp": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^4.22.1",
    "chai": "^4.3.4",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-oclif": "^0.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "ts-node": "^10.4.0",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "oclif": {
    "default": ".",
    "commands": "dist/src/commands",
    "bin": "rehearsal",
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ],
    "plugins": [
      "@oclif/plugin-version"
    ]
  },
  "volta": {
    "node": "16.16.0",
    "yarn": "1.22.19"
  }
}
