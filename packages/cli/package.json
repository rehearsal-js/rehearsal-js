{
  "name": "@rehearsal/cli",
  "version": "0.0.36",
  "description": "Rehearsal CLI",
  "repository": {
    "type": "git",
    "url": "https://github.com/rehearsal-js/rehearsal-js.git"
  },
  "license": "BSD-2-Clause",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "rehearsal": "./bin/rehearsal.js"
  },
  "packageManager": "pnpm@7.12.1",
  "files": [
    "bin",
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -b",
    "docs": "api-extractor run --typescript-compiler-folder ./node_modules/typescript && api-documenter markdown -i ./temp -o ./docs",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint --fix . --ext .ts",
    "lint:tsc-src": "tsc --noEmit",
    "prepare": "pnpm build",
    "test": "vitest --run",
    "test:watch": "vitest --coverage --watch"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@microsoft/api-documenter": "^7.13.68",
    "@rehearsal/migrate": "workspace:*",
    "@rehearsal/reporter": "workspace:*",
    "@rehearsal/upgrade": "workspace:*",
    "@rehearsal/migration-graph": "workspace:*",
    "commander": "^9.4.0",
    "compare-versions": "^5.0.1",
    "debug": "^4.3.2",
    "execa": "^5.1.1",
    "findup-sync": "^5.0.0",
    "fs-extra": "^10.0.0",
    "glob": "^8.0.3",
    "json5": "^2.2.0",
    "listr2": "^5.0.1",
    "semver": "^7.3.5",
    "simple-git": "^3.10.0",
    "tmp": "^0.2.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "typescript": "4.7.4",
    "vitest": "^0.23.4"
  },
  "engines": {
    "node": ">=14.16.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
