{
  "name": "@rehearsal/utils",
  "version": "1.0.2-beta",
  "description": "Rehearsal Utils",
  "keywords": [
    "rehearsal",
    "ast",
    "ast traversal",
    "transforms",
    "tsc utility",
    "tsc utils",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rehearsal-js/rehearsal-js.git"
  },
  "license": "BSD-2-Clause",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint --fix . --ext .ts",
    "lint:tsc-src": "tsc --noEmit",
    "lint:tsc-test": "tsc --noEmit --project test/tsconfig.json",
    "test": "vitest --run",
    "test:watch": "vitest --coverage --watch"
  },
  "devDependencies": {
    "vitest": "^0.23.4"
  },
  "peerDependencies": {
    "typescript": ">4.9"
  },
  "packageManager": "pnpm@7.12.1",
  "engines": {
    "node": ">=14.16.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
