{
  "name": "@rehearsal/migration-graph-ember",
  "version": "0.0.36",
  "description": "Tool for generating a typescript migration graph for a ember.js",
  "license": "BSD-2-Clause",
  "packageManager": "pnpm@7.12.1",
  "scripts": {
    "build": "tsc -b",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint --fix . --ext .ts",
    "lint:tsc-src": "tsc --noEmit",
    "prepare": "pnpm build",
    "test": "vitest --run"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@rehearsal/migration-graph-shared": "^0.0.36",
    "@babel/generator": "^7.18.13",
    "@babel/parser": "^7.18.13",
    "@babel/traverse": "^7.18.13",
    "fast-glob": "^3.2.12",
    "find-up": "^5.0.0",
    "fs-extra": "^10.1.0",
    "micromatch": "^4",
    "prettier": "^2.7.0",
    "resolve-package-path": "^4.0.3",
    "sort-package-json": "^1"
  },
  "devDependencies": {
    "@babel/types": "^7.18.13",
    "fixturify": "^2",
    "tmp": "^0.2.1",
    "vitest": "^0.23.4",
    "walk-sync": "^3"
  },
  "peerDependencies": {
    "typescript": ">4.7"
  },
  "volta": {
    "extends": "../../package.json"
  }
