{
  "name": "@rehearsal/migration-graph-ember",
  "version": "3.0.0-beta",
  "description": "Tool for generating a typescript migration graph for a ember.js",
  "license": "BSD-2-Clause",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "lint": "npm-run-all lint:*",
    "lint:tsc-src": "tsc --noEmit",
    "lint:tsc-test": "tsc --noEmit --project test/tsconfig.json",
    "test": "vitest --run --config ./vitest.config.ts --coverage",
    "test:slow": "vitest --run --config ./vitest.config.slow.ts",
    "version": "pnpm version"
  },
  "resolutions": {
    "@swc/core": "1.3.44"
  },
  "dependencies": {
    "@glimmer/syntax": "^0.84.3",
    "@rehearsal/migration-graph-shared": "workspace:*",
    "@swc/core": "^1.3.46",
    "babel-plugin-htmlbars-inline-precompile": "^5.3.1",
    "debug": "^4.3.4",
    "enhanced-resolve": "^5.12.0",
    "fast-glob": "^3.2.12",
    "find-up": "^6.3.0",
    "type-fest": "^3.8.0"
  },
  "devDependencies": {
    "@rehearsal/test-support": "workspace:*",
    "@types/lodash.merge": "^4.6.7",
    "@vitest/coverage-c8": "^0.30.1",
    "enhanced-resolve": "^5.12.0",
    "fixturify": "^3.0.0",
    "fixturify-project": "^5.2.0",
    "lodash.merge": "^4.6.2",
    "tmp": "^0.2.1",
    "vitest": "^0.30.0"
  },
  "peerDependencies": {
    "typescript": "^5.0"
  },
  "packageManager": "pnpm@8.2.0",
  "engines": {
    "node": ">=14.16.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
