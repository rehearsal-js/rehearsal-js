export class Foo {
  value = 0;

  get foo() {
    return this.value;
  }

  set foo(value) {
    this.value = value;
  }

  boo(value: number) {
    this.value = value;

    if (this.value) {
      return null;
    } else {
      return this.value;
    }
  }
}
