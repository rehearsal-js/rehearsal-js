{
  "name": "@rehearsal/upgrade",
  "version": "0.0.28",
  "description": "Rehearsal Dependency Upgrade Tool",
  "keywords": [
    "upgrade",
    "dependency",
    "dependencies",
    "rehearsal",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rehearsal-js/rehearsal-js.git"
  },
  "license": "BSD-2-Clause",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -b",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint --fix . --ext .ts",
    "lint:tsc-src": "tsc --noEmit",
    "prepare": "yarn build",
    "test": "vitest --run",
    "test:watch": "vitest --coverage --watch"
  },
  "dependencies": {
    "@rehearsal/reporter": "^0.0.28",
    "@rehearsal/service": "^0.0.28",
    "@rehearsal/plugins": "^0.0.28",
    "@rehearsal/utils": "^0.0.28",
    "commander": "^9.4.0",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/eslint": "^8.4.1",
    "@types/node": "^18.0.3",
    "@types/react": "^18.0.9",
    "vitest": "^0.21.1"
  },
  "peerDependencies": {
    "typescript": ">4.7"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "volta": {
    "node": "16.16.0",
    "yarn": "1.22.19"
  }
}
