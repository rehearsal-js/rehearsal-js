{
  "name": "@rehearsal/codefixes",
  "version": "1.0.1",
  "description": "Rehearsal Dependency Codefixes Collection",
  "keywords": [
    "transforms",
    "codefixes",
    "dependency",
    "dependencies",
    "rehearsal",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rehearsal-js/rehearsal-js.git"
  },
  "license": "BSD-2-Clause",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "lint": "npm-run-all lint:*",
    "lint:tsc-src": "tsc --noEmit",
    "lint:tsc-test": "tsc --noEmit --project test/tsconfig.json",
    "test": "vitest --run --config ./vitest.config.ts --coverage",
    "test:watch": "vitest --coverage --watch",
    "version": "pnpm version"
  },
  "dependencies": {
    "@rehearsal/ts-utils": "workspace:*",
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@rehearsal/migrate": "workspace:*",
    "@rehearsal/reporter": "workspace:*",
    "@rehearsal/service": "workspace:*",
    "@types/prettier": "^2.7.2",
    "@vitest/coverage-c8": "^0.30.1",
    "fixturify": "^3.0.0",
    "fixturify-project": "^5.2.0",
    "fs-extra": "^11.1.1",
    "prettier": "^2.8.7",
    "tmp": "^0.2.1",
    "vitest": "^0.30.0"
  },
  "peerDependencies": {
    "typescript": "^5.0"
  },
  "packageManager": "pnpm@8.2.0",
  "engines": {
    "node": ">=14.16.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
