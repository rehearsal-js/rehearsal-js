{
  "name": "@rehearsal/plugins",
  "version": "0.0.33",
  "description": "Rehearsal JavaScript to TypeScript Shared Libraries",
  "keywords": [
    "rehearsal",
    "plugins",
    "transforms",
    "javascript",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rehearsal-js/rehearsal-js.git"
  },
  "license": "BSD-2-Clause",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "run -T tsc -b",
    "lint": "run -T run-p 'lint:*'",
    "lint:eslint": "run -T eslint --fix . --ext .ts",
    "lint:tsc-src": "run -T tsc --noEmit",
    "prepack": "yarn build",
    "test": "run -T vitest --run",
    "test:watch": "run -T vitest --coverage --watch"
  },
  "dependencies": {
    "@rehearsal/codefixes": "workspace:*",
    "@rehearsal/service": "workspace:*",
    "eslint": "8.22.0"
  },
  "devDependencies": {
    "@rehearsal/reporter": "workspace:*",
    "@rehearsal/utils": "workspace:*"
  },
  "peerDependencies": {
    "typescript": "*"
  },
  "engines": {
    "node": ">=14.16.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
