{
  "name": "@rehearsal/plugins",
  "version": "0.0.36",
  "description": "Rehearsal JavaScript to TypeScript Shared Libraries",
  "keywords": [
    "rehearsal",
    "plugins",
    "transforms",
    "javascript",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rehearsal-js/rehearsal-js.git"
  },
  "license": "BSD-2-Clause",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -b",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint --fix . --ext .ts",
    "lint:tsc-src": "tsc --noEmit",
    "prepare": "pnpm build",
    "test": "vitest --run",
    "test:watch": "vitest --coverage --watch"
  },
  "dependencies": {
    "@rehearsal/codefixes": "workspace:*",
    "@rehearsal/service": "workspace:*",
    "@rehearsal/utils": "workspace:*",
    "@rehearsal/reporter": "workspace:*",
    "eslint": "8.22.0"
  },
  "devDependencies": {
    "vitest": "^0.22.1",
    "fixturify-project": "^5.0.3",
    "@types/eslint": "^8.4.1"
  },
  "peerDependencies": {
    "typescript": ">4.7"
  },
  "engines": {
    "node": ">=14.16.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
