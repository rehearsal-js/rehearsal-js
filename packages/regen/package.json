{
  "name": "@rehearsal/regen",
  "version": "3.0.2-beta",
  "description": "Rehearsal JavaScript to TypeScript migration status reporting tool",
  "keywords": [
    "rehearsal",
    "migrate",
    "migration",
    "javascript",
    "typescript",
    "reporting"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rehearsal-js/rehearsal-js.git"
  },
  "license": "BSD-2-Clause",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -b",
    "lint": "npm-run-all lint:*",
    "lint:tsc-src": "tsc --noEmit",
    "lint:tsc-test": "tsc --noEmit --project test/tsconfig.json",
    "test": "vitest --run --config ./vitest.config.ts --coverage",
    "test:watch": "vitest --coverage --watch",
    "version": "pnpm version"
  },
  "resolutions": {
    "listr2": "^5.0"
  },
  "dependencies": {
    "@rehearsal/plugins": "workspace:*",
    "@rehearsal/reporter": "workspace:*",
    "@rehearsal/service": "workspace:*",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.30.1",
    "listr2": "^5.0.8",
    "vitest": "^0.30.0"
  },
  "peerDependencies": {
    "eslint": "^8.40.0",
    "typescript": "^5.0.0"
  },
  "packageManager": "pnpm@8.2.0",
  "engines": {
    "node": ">=14.16.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
