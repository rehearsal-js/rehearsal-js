{
  "name": "@rehearsal/migration-graph",
  "version": "1.0.5-beta",
  "description": "Tool for generating a typescript conversion graph",
  "license": "BSD-2-Clause",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "tsc -b",
    "clean": "rm -rf fixtures/ember/*/node_modules",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint --fix . --ext .ts",
    "lint:tsc-src": "tsc --noEmit",
    "lint:tsc-test": "tsc --noEmit --project test/tsconfig.json",
    "test": "vitest --run",
    "version": "pnpm version"
  },
  "dependencies": {
    "@rehearsal/migration-graph-ember": "workspace:*",
    "@rehearsal/migration-graph-shared": "workspace:*",
    "debug": "^4.3.4",
    "fs-extra": "^10.1.0",
    "minimatch": "^5.1.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@rehearsal/test-support": "workspace:*",
    "@types/minimatch": "^5.1.2",
    "vitest": "^0.23.4"
  },
  "peerDependencies": {
    "typescript": "^4.9 || ^5.0"
  },
  "packageManager": "pnpm@7.12.1",
  "volta": {
    "extends": "../../package.json"
  }
}
