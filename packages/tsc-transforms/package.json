{
  "name": "@rehearsal/tsc-transforms",
  "version": "0.0.13",
  "description": "Rehearsal TSC Transforms",
  "keywords": [
    "rehearsal",
    "ast",
    "transforms",
    "ts-migrate",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rehearsal-js/rehearsal-js.git"
  },
  "license": "BSD-2-Clause",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn tsc -b",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "yarn eslint . --ext .ts",
    "lint:tsc-src": "yarn tsc --noEmit",
    "prepare": "yarn build",
    "test": "yarn lint && yarn nyc mocha"
  },
  "nyc": {
    "extension": [
      ".ts"
    ]
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "recast": "^0.20.5",
    "ts-node": "^10.4.0",
    "chai": "^4.3.6",
    "mocha": "^9.2.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "volta": {
    "node": "16.14.0",
    "yarn": "1.22.17"
  }
}
