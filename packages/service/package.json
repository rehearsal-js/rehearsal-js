{
  "name": "@rehearsal/service",
  "version": "1.0.12-beta",
  "description": "Rehearsal Service",
  "keywords": [
    "rehearsal",
    "service",
    "ast",
    "transforms",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rehearsal-js/rehearsal-js.git"
  },
  "license": "BSD-2-Clause",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "lint": "npm-run-all lint:*",
    "lint:tsc-src": "tsc --noEmit",
    "lint:tsc-test": "tsc --noEmit --project test/tsconfig.json",
    "test": "vitest --run",
    "test:watch": "vitest --coverage --watch",
    "version": "pnpm version"
  },
  "dependencies": {
    "@glimmer/component": "^1.1.2",
    "@glint/core": "1.0.0-beta.4",
    "@glint/environment-ember-loose": "1.0.0-beta.3",
    "@glint/environment-ember-template-imports": "1.0.0-beta.3",
    "@glint/template": "1.0.0-beta.3",
    "@rehearsal/reporter": "workspace:*",
    "@rehearsal/test-support": "workspace:*",
    "@rehearsal/utils": "workspace:*",
    "ember-template-imports": "^3.4.1",
    "findup-sync": "^5.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "vitest": "^0.28.5"
  },
  "peerDependencies": {
    "typescript": "^4.9 || ^5.0"
  },
  "packageManager": "pnpm@7.12.1",
  "engines": {
    "node": ">=14.16.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
