{
  "name": "@rehearsal/service",
  "version": "1.0.5-beta",
  "description": "Rehearsal Service",
  "keywords": [
    "rehearsal",
    "service",
    "ast",
    "transforms",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rehearsal-js/rehearsal-js.git"
  },
  "license": "BSD-2-Clause",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint --fix . --ext .ts",
    "lint:tsc-src": "tsc --noEmit",
    "lint:tsc-test": "tsc --noEmit --project test/tsconfig.json",
    "test": "vitest --run",
    "test:watch": "vitest --coverage --watch",
    "version": "pnpm version"
  },
  "dependencies": {
    "@rehearsal/reporter": "workspace:*",
    "@rehearsal/utils": "workspace:*",
    "findup-sync": "^5.0.0",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "vitest": "^0.23.4"
  },
  "peerDependencies": {
    "typescript": "^4.9 || ^5.0"
  },
  "packageManager": "pnpm@7.12.1",
  "engines": {
    "node": ">=14.16.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
