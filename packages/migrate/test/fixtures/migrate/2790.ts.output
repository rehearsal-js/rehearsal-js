class Student {
  name: string;
  grade: number;
  gender?: string;

  constructor({ name, grade, gender }: { name: string; grade: number; gender: string }) {
    this.name = name;
    this.grade = grade;
    this.gender = gender;
  }
}
const student = new Student({ name: '', grade: 3, gender: 'male' });
delete student.gender;
