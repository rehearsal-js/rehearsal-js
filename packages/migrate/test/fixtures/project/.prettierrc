{
  "printWidth": 100,
  "singleQuote": false,
  "trailingComma": "es5",
  "overrides": [
    {
      "files": "*.hbs",
      "options": {
        "parser": "glimmer"
      }
    },
    {
      "files": "*.gjs, *.gts",
      "options": {
        "parser": "glimmer"
      }
    }
  ],
  "plugins": ["prettier-plugin-ember-template-tag"]
}
