{
  "name": "rehearsal-workspace",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rehearsal-js/rehearsal-js.git"
  },
  "scripts": {
    "lint": "pnpm lint:package-json && pnpm recursive run lint",
    "lint:package-json": "sort-package-json package.json packages/*/package.json",
    "prepare": "pnpm recursive run prepare",
    "test": "pnpm recursive run test",
    "changelog": "pnpm conventional-changelog -i CHANGELOG.md -s",
    "clean": "find . -name 'node_modules' -o -name 'dist' -o -name 'coverage' -type d -prune -exec rm -rf '{}' +",
    "reset": "pnpm clean && pnpm install"
  },
  "dependencies": {
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@babel/types": "^7.18.13",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.2",
    "@microsoft/api-documenter": "^7.13.68",
    "@microsoft/api-extractor": "^7.18.19",
    "@rehearsal/reporter": "workspace:*",
    "@rehearsal/utils": "workspace:*",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.18.0",
    "@types/commander": "^2.12.2",
    "@types/debug": "^4.1.7",
    "@types/eslint": "^8.4.1",
    "@types/findup-sync": "^4.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^8.0.0",
    "@types/json5": "*",
    "@types/lodash.get": "^4.4.7",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^18.0.3",
    "@types/prettier": "^2.6.3",
    "@types/react": "^18.0.9",
    "@types/sarif": "^2.1.4",
    "@types/semver": "^7.3.9",
    "@types/tmp": "^0.2.3",
    "@types/toposort": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vitest/coverage-c8": "^0.22.1",
    "commander": "^9.4.0",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "fixturify": "^2",
    "fixturify-project": "^5.0.3",
    "husky": "^8.0.1",
    "lodash.get": "^4.4.2",
    "micromatch": "^4",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "sort-package-json": "^1.57.0",
    "tmp": "^0.2.1",
    "ts-node": "^10.4.0",
    "typescript": "4.7.4",
    "vite": "^3.1.3",
    "vitest": "^0.23.4",
    "walk-sync": "^3"
  },
  "packageManager": "pnpm@7.12.1",
  "volta": {
    "node": "16.17.0"
  }
}
